{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run eToro",
            "type": "dart",
            "request": "launch",
            "program": "bin/main.dart",
            "env": {
                "LOG_LEVEL": "FINEST"
            },
            "args": [
                "etoro",
                "--file",
                "${workspaceFolder}${pathSeparator}.sample_data${pathSeparator}etoro.csv",
                "--exchange-directory",
                "${workspaceFolder}${pathSeparator}data${pathSeparator}exchange",
                "-l",
                "finest"
            ]
        },
        {
            "name": "Run Global Help",
            "type": "dart",
            "request": "launch",
            "program": "bin/main.dart",
            "args": [
                "--help"
            ]
        },
        {
            "name": "Run eToro Help",
            "type": "dart",
            "request": "launch",
            "program": "bin/main.dart",
            "args": [
                "help",
                "etoro"
            ]
        },
        {
            "name": "Run test",
            "type": "dart",
            "request": "launch",
            "program": "test/all.dart"
        }
    ]
}